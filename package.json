    "@types/react-dom": "^18.2.18",
    "i18next": "^23.7.11",
    "react-i18next": "^13.5.0",
    "typescript": "^5.1.3"
  },
  "homepage": "https://github.com/AChristoff/i18n-type-safe#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AChristoff/i18n-type-safe.git"
  },
  "bugs": {
    "url": "https://github.com/AChristoff/i18n-type-safe/issues"
